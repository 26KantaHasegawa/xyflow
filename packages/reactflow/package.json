  "module": "dist/reactflow.esm.js",
  "types": "dist/reactflow.cjs.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wbkd/react-flow.git",
    "directory": "packages/reactflow"
  },
  "scripts": {
